{
  "openapi": "3.0.3",
  "info": {
    "title": "Korean Real Estate MCP API",
    "description": "한국 부동산 실거래가 데이터 조회 및 분석을 위한 MCP (Model Context Protocol) API 서버입니다. 국토교통부 실거래가 공개시스템, 한국은행 ECOS, 한국부동산원 R-ONE 등의 공공 API를 통합하여 제공합니다.",
    "version": "0.1.0",
    "contact": {
      "name": "Changoo Lee",
      "email": "lchangoo@gmail.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "servers": [
    {
      "url": "https://localhost:8000",
      "description": "Local development server"
    }
  ],
  "tags": [
    {
      "name": "apartment",
      "description": "아파트 실거래가 관련 API"
    },
    {
      "name": "officetel",
      "description": "오피스텔 실거래가 관련 API"
    },
    {
      "name": "row-house",
      "description": "연립/다세대 실거래가 관련 API"
    },
    {
      "name": "single-house",
      "description": "단독/다가구 실거래가 관련 API"
    },
    {
      "name": "commercial",
      "description": "상업업무용 부동산 실거래가 관련 API"
    },
    {
      "name": "industrial",
      "description": "공장/창고 등 산업용 부동산 실거래가 관련 API"
    },
    {
      "name": "land",
      "description": "토지 실거래가 관련 API"
    },
    {
      "name": "region",
      "description": "법정동 코드 조회 API"
    },
    {
      "name": "ecos",
      "description": "한국은행 ECOS 경제통계 API"
    },
    {
      "name": "reb",
      "description": "한국부동산원 R-ONE API"
    }
  ],
  "paths": {
    "/api/v1/apartment/trade": {
      "get": {
        "tags": ["apartment"],
        "summary": "아파트 매매 실거래가 조회",
        "description": "지정된 지역의 아파트 매매 실거래가 데이터를 조회합니다.",
        "parameters": [
          {
            "name": "lawd_cd",
            "in": "query",
            "required": true,
            "description": "법정동코드 5자리",
            "schema": {
              "type": "string",
              "pattern": "^[0-9]{5}$",
              "example": "11680"
            }
          },
          {
            "name": "deal_ymd",
            "in": "query",
            "required": true,
            "description": "거래년월 (YYYYMM 형식)",
            "schema": {
              "type": "string",
              "pattern": "^[0-9]{6}$",
              "example": "202506"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "성공",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TradeResponse"
                }
              }
            }
          },
          "400": {
            "description": "잘못된 요청"
          },
          "500": {
            "description": "서버 오류"
          }
        }
      }
    },
